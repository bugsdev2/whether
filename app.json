{
    "expo": {
        "name": "Cloud Compass",
        "slug": "cloudcompass",
        "version": "0.0.2",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#292929"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "com.compass.cloud",
            "versionCode": 3,
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "d0bf0952-418c-4215-afb4-3f4cf427d6a3"
            }
        }
    }
}
